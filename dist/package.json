{
  "name": "@saleor/auth-sdk",
  "version": "0.9.0",
  "description": "Auth SDK for Saleor",
  "types": "index.d.ts",
  "main": "index.js",
  "author": "Saleor Team",
  "license": "BSD-3-Clause",
  "dependencies": {
    "cookie": "^0.5.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6"
  },
  "peerDependencies": {
    "@apollo/client": "^3.7.15",
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "urql": "^4.0.3"
  },
  "peerDependenciesMeta": {
    "@apollo/client": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "urql": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "exports": {
    "./package.json": "./package.json",
    "./react/apollo": {
      "types": "./react/apollo.d.ts",
      "require": "./react/apollo.js",
      "default": "./react/apollo.js"
    },
    "./react/urql": {
      "types": "./react/urql.d.ts",
      "import": "./react/urql.mjs",
      "require": "./react/urql.js"
    },
    "./react": {
      "types": "./react/index.d.ts",
      "import": "./react/index.mjs",
      "require": "./react/index.js"
    },
    "./next": {
      "types": "./next/index.d.ts",
      "import": "./next/index.mjs",
      "require": "./next/index.js"
    },
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saleor/auth-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/saleor/auth-sdk/issues"
  },
  "homepage": "https://github.com/saleor/auth-sdk#readme"
}
